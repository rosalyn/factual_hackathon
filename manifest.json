{
  "manifest_version": 2,
  "name": "Regex Binoculars",
  "description": "This extension allows search using regular expressions in a browser.",
  "version": "1.0",
  "permissions": [
    "<all_urls>"
  ],
  "icons": {
    "16": "rosalyn16.png",
    "48": "rosalyn48.png",
    "128": "rosalyn128.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "css": ["rosalynstyle.css"],
      "js": ["run.js"]
    }
  ]
}
